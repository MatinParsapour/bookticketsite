<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>ceoValidation</servlet-name>
    <servlet-class>servlets.ceoservlets.CEOCodeValidation</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>ceoEnter</servlet-name>
    <servlet-class>servlets.ceoservlets.CEOEnterServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>CustomerEnterServlet</servlet-name>
    <servlet-class>servlets.customerservlets.CustomerEnterServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>CustomerEnterServlet</servlet-name>
    <url-pattern>/customerEnter</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ceoValidation</servlet-name>
    <url-pattern>/ceoValidation</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ceoEnter</servlet-name>
    <url-pattern>/ceoEnter</url-pattern>
  </servlet-mapping>

</web-app>
